{
  "name": "gatsby-demo-simple-auth",
  "description": "Gatsby demo of simplified authentication flow.",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "start": "npm run develop",
    "compile": "truffle compile",
    "deploy:local": "truffle migrate --reset --network localhost",
    "deploy:dev": "truffle migrate --reset --network development",
    "parser": "babel-eslint",
    "pretest": "eslint --ignore-path .gitignore .",
    "lint": "eslint ./**.js **.js",
    "travislint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint . --fix",
    "lint:sol": "solium -d contracts/"
  },
  "author": "nexty.io",
  "dependencies": {
    "antd": "^4.0.0",
    "big-integer": "^1.6.43",
    "big-number": "^2.0.0",
    "bignumber.js": "^8.1.1",
    "gatsby": "^2.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.1.30",
    "lodash": "^4.17.11",
    "node-sass": "^4.13.1",
    "prop-types": "^15.6.1",
    "react": "^16.13.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.2.0",
    "truffle": "^5.0.12",
    "truffle-hdwallet-provider": "^1.0.6",
    "web3": "1.0.0-beta.36"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT"
}
